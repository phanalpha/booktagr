{
  "manifest_version": 3,
  "name": "Booktagr",
  "description": "Tag your bookmarks",
  "version": "0.1.0",
  "icons": {
    "128": "booktagr.png"
  },
  "permissions": [
    "bookmarks",
    "favicon"
  ],
  "web_accessible_resources": [
    {
      "resources": ["_favicon/*"],
      "matches": ["<all_urls>"],
      "extension_ids": ["*"]
    }
  ],
  "action": {
    "default_popup": "index.html"
  }
}
